pipeline {
    agent any

    stages {
        stage('Clone Application Code') {
            steps {
                echo 'Cloning APplication Code from GITHUB'
            }
        }
        stage('Build') {
            steps {
                echo 'Build Application Code'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy Application Code'
            }
        }
    }
}
